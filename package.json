{
	"name": "redis-learn",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"dev": "nodemon src/index.ts",
		"build": "rimraf dist && tsc",
		"start": "node dist/index.js",
		"ts.check": "tsc --project tsconfig.json",
		"add-build": "git add dist",
		"push-build": "git add . && git commit --amend --no-edit && git push && git pull && echo 'Pushed build'"
	},
	"pre-commit": [
		"ts.check",
		"build",
		"add-build"
	],
	"post-commit": [
		"push-build"
	],
	"devDependencies": {
		"@types/express": "^4.17.13",
		"@types/node": "^17.0.40",
		"nodemon": "^2.0.16",
		"post-commit": "^0.0.1",
		"pre-commit": "^1.2.2",
		"rimraf": "^3.0.2",
		"ts-node": "^10.8.1",
		"typescript": "^4.7.3"
	},
	"dependencies": {
		"axios": "^0.27.2",
		"express": "^4.18.1",
		"redis": "^4.1.0"
	}
}
